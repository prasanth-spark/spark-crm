{
    "name": "employecrm/admin",
    "description": "",
    "keywords": [
        "employecrm",
        "admin"
    ],
    "homepage": "https://github.com/employecrm/admin",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "hariharan",
            "email": "hariharan@sparkouttech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Employecrm\\Admin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Employecrm\\Admin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Employecrm\\Admin\\AdminServiceProvider"
            ],
            "aliases": {
                "Admin": "Employecrm\\Admin\\AdminFacade"
            }
        }
    }
}
